{
  "version": 2,
  "framework": null,
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.4"
    }
  },
  "routes": [
    { "src": "/build/(.*)", "dest": "/public/build/$1" },
    { "src": "/images/(.*)", "dest": "/public/images/$1" },
    { "src": "/background/(.*)", "dest": "/public/background/$1" },
    { "src": "/storage/(.*)", "dest": "/public/storage/$1" },
    { "src": "/favicon.ico", "dest": "/public/favicon.ico" },
    { "src": "/apple-touch-icon.png", "dest": "/public/apple-touch-icon.png" },
    { "src": "/android-chrome-192x192.png", "dest": "/public/android-chrome-192x192.png" },
    { "src": "/android-chrome-512x512.png", "dest": "/public/android-chrome-512x512.png" },
    { "src": "/favicon-16x16.png", "dest": "/public/favicon-16x16.png" },
    { "src": "/favicon-32x32.png", "dest": "/public/favicon-32x32.png" },
    { "src": "/logo.png", "dest": "/public/logo.png" },
    { "src": "/placeholder.svg", "dest": "/public/placeholder.svg" },
    { "src": "/robots.txt", "dest": "/public/robots.txt" },
    { "src": "/api/(.*)", "dest": "/api/index.php" },
    { "src": "/(.*)",  "dest": "/api/index.php" }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "APP_URL": "https://sccd.vercel.app",
    "APP_KEY": "base64:NySra04sfHpoMOX9n7lzELcBsubNvpihwEShgnxcGGw=",
    "APP_CONFIG_CACHE": "/tmp/config.php",
    "APP_EVENTS_CACHE": "/tmp/events.php",
    "APP_PACKAGES_CACHE": "/tmp/packages.php",
    "APP_ROUTES_CACHE": "/tmp/routes.php",
    "APP_SERVICES_CACHE": "/tmp/services.php",
    "VIEW_COMPILED_PATH": "/tmp",
    "CACHE_DRIVER": "array",
    "LOG_CHANNEL": "stderr",
    "SESSION_DRIVER": "cookie"
  }
}
